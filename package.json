{
  "name": "vinckr.com",
  "version": "1.0.0",
  "description": "This is my personal website, there are many like it but this one is mine.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "npx @11ty/eleventy && tailwindcss -i styles/tailwind.css -o _site/styles/tailwind.css --watch",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -i styles/tailwind.css -o _site/styles/tailwind.css --watch --postcss",
    "test": "npx broken-link-checker http://localhost:8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinckr/vinckr.com.git"
  },
  "keywords": [],
