  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vinckr/vinckr.com/issues"
  },
  "homepage": "https://github.com/vinckr/vinckr.com#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2"
  }
}
